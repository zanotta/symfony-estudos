{# templates/lucky/number.html.twig #}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}
            <h1>Hi {{ name }}, your lucky number is {{ number }}</h1>
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>

